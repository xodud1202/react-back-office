  "private": true,
  "scripts": {
    "build": "next build",
    "lint": "eslint pages components hooks utils store types --ext .js,.jsx,.ts,.tsx",
    "dev": "cross-env PORT=3011 next dev",
    "start": "cross-env PORT=3011 next start",
    "sass:build": "sass --source-map public/assets/scss/style.scss public/assets/css/style.css",
